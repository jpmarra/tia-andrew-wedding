{
    "name": "gatsby-contentful-starter",
    "description": "Starter Contentful Gatsby Blog",
    "version": "1.0.0",
    "author": "Stefan Judis <stefanjudis@gmail.com>",
    "bugs": {
        "url": "https://github.com/contentful-userland/gatsby-contentful-starter/issues"
    },
    "browserslist": [
        ">0.2%",
        "not ie 11",
        "not op_mini all"
    ],
    "dependencies": {
        "babel-plugin-styled-components": "^1.10.0",
        "detect-browser": "^4.6.0",
        "gatsby-background-image": "^0.8.0",
        "gatsby-image": "^2.2.6",
        "gatsby-plugin-manifest": "^2.2.3",
        "gatsby-plugin-react-helmet": "^3.1.2",
        "gatsby-plugin-react-svg": "^2.1.1",
        "gatsby-plugin-sharp": "^2.2.8",
        "gatsby-plugin-styled-components": "^3.1.2",
        "gatsby-plugin-web-font-loader": "^1.0.4",
        "gatsby-source-contentful": "^2.1.14",
        "gatsby-transformer-remark": "^2.6.7",
        "gatsby-transformer-sharp": "^2.2.4",
        "lodash": "^4.17.11",
        "react": "^16.8.5",
        "react-dom": "^16.8.5",
        "react-helmet": "^5.2.0",
        "sharp": "^0.21.0",
        "styled-components": "^4.2.0"
    },
    "devDependencies": {
        "babel-eslint": "^10.0.1",
        "babel-plugin-styled-components": "^1.10.6",
        "chalk": "^2.4.1",
        "contentful-import": "^7.4.0",
        "eslint": "^5.6.1",
        "eslint-plugin-react": "^7.11.1",
        "gatsby": "^2.2.5",
        "gh-pages": "^2.0.1",
        "inquirer": "^6.2.0",
        "lodash": "^4.17.15",
        "normalize.css": "^8.0.1",
        "prettier": "^1.14.3",
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "react-helmet": "^5.2.1",
        "react-spring": "^8.0.27",
        "sharp": "^0.21.0",
        "styled-components": "^4.3.2"
    },
    "homepage": "https://github.com/contentful-userland/gatsby-contentful-starter#readme",
    "keywords": [
        "gatsby",
        "contentful"
    ],
    "license": "MIT",
    "main": "n/a",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/contentful-userland/gatsby-contentful-starter.git"
    },
    "scripts": {
        "dev": "gatsby develop",
        "lint": "eslint --ext .js,.jsx --ignore-pattern public .",
        "test": "echo \"Error: no test specified\" && exit 1",
        "format": "prettier --trailing-comma es5 --no-semi --single-quote --write 'src/**/*.js' 'src/**/*.md' 'bin/*.js'",
        "build": "gatsby build",
        "deploy": "gatsby build --prefix-paths && gh-pages -d public",
        "fix-semi": "eslint --quiet --ignore-pattern node_modules --ignore-pattern public --parser babel-eslint --no-eslintrc --rule '{\"semi\": [2, \"never\"], \"no-extra-semi\": [2]}' --fix *.js bin/*.js",
        "postinstall": "node ./bin/hello.js",
        "setup": "node ./bin/setup.js",
        "heroku-postbuild": "gatsby build",
        "cleanup-repository": "rimraf bin contentful && yarn remove contentful-import chalk inquirer rimraf "
    }
}
